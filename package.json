{
  "name": "meal-booking-app",
  "version": "1.0.0",
  "description": "Meal Booking Ecommerce App",
  "main": "api/index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "nodemon --exec babel-node api/index.js",
    "test": "nyc mocha -r esm --recursive -t 5000 --exit",
    "build": "npm run clean && npm run build-babel && npm run dulpicate-swagger-docs && npm audit fix",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./api -s",
    "dulpicate-swagger-docs": "cat ./api/swagger.json >> ./build/swagger.json",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "heroku-run-build-script": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timzprof/book-a-meal.git"
  },
  "author": "Timilehin Olumofin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timzprof/book-a-meal/issues"
  },
  "homepage": "https://github.com/timzprof/book-a-meal#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/polyfill": "^7.2.5",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "esm": "^3.2.4",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "swagger-ui-express": "^4.0.2",
    "trim-request": "^1.0.6"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.2",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-fs": "^4.8.0",
    "nodemon": "^1.18.9",
    "nyc": "13.3.0",
    "prettier": "^1.16.4"
  }
}
